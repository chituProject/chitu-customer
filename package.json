{
  "name": "offpay-customer",
  "version": "1.0.0",
  "description": "offpay frontend for customer",
  "author": "BruceWang <bruce@offpay.cn>",
  "private": true,
  "scripts": {
    "dev": "cat ./utils/config-dev.json > ./utils/config.json",
    "copy:prod": "cat ./utils/config-prod.json > ./utils/config.json",
    "start": "node build/dev-server.js",
    "build": "cat ./src/utils/config-dev.json > ./src/utils/config.json && node build/build.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint": "eslint --ext .js,.vue pages utils components store --fix"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "suppress-exit-code eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "iconv-lite": "^0.4.23",
    "pingpp-js": "^2.2.6",
    "qrcode": "git://github.com/noteScript/qrcodejs.git#master",
    "uuid-js": "^0.7.5",
    "vue": "^2.6.10",
    "vuex": "^3.1.1",
    "vxe-table": "^2.9.11",
    "xe-utils": "^2.4.5"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^%/(.*)$": "<rootDir>/static/$1"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "flush-promises": "^1.0.2",
    "husky": "^2.5.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^8.2.1",
    "prettier": "1.17.1",
    "suppress-exit-code": "^0.1.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  }
}
